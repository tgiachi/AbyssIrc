[req]
default_bits = 2048
prompt = no
default_md = sha256
req_extensions = req_ext
distinguished_name = dn
x509_extensions = v3_ca

[ dn ]
C = IT
ST = Florence
L = Florence
O = AbyssIRC
OU = IRC
CN = irc.abyssirc.net

[ req_ext ]
subjectAltName = @alt_names

[ v3_ca ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = irc.abyssirc.net
